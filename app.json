{
    "expo": {
        "name": "Medishots",
        "slug": "medishots",
        "version": "1.0.0",
        "orientation": "portrait",
        "scheme": "medishots",
        "userInterfaceStyle": "automatic",
        "newArchEnabled": true,
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "com.betoatexpo.medishotapp",
            // "appleTeamId": "T2A8YY9YDW",
            "appleTeamId": "S6GL3VTAFF",
            "icon": "./assets/icon.icon"
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/images/adaptive-icon.png",
                "backgroundColor": "#ffffff"
            },
            "edgeToEdgeEnabled": true,
            "package": "com.betoatexpo.medishotapp"
        },
        "web": {
            "bundler": "metro",
            "output": "static",
            "favicon": "./assets/images/favicon.png"
        },
        "plugins": [
            "expo-router",
            [
                "expo-splash-screen",
                {
                    "image": "./assets/images/splash-icon.png",
                    "imageWidth": 200,
                    "resizeMode": "contain",
                    "backgroundColor": "#ffffff"
                }
            ],
            "expo-font",
            "expo-web-browser",
            "expo-video"
        ],
        "experiments": {
            "typedRoutes": true
        }
    }
}
